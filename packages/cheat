#!/usr/bin/env bash

# https://github.com/cheat/cheat

dra download -i -s "cheat-linux-amd64.gz" -o ~/.local/bin/cheat cheat/cheat

cheat --completion bash > ~/.local/share/bash-completion/completions/cheat
cheat --completion zsh > ~/.local/share/zsh-completion/_cheat
cheat --completion fish > ~/.config/fish/completions/cheat.fish
